    lr_mult: 10
    decay_mult: 10
  }
  param {
    lr_mult: 20
    decay_mult: 10
  }
  inner_product_param {
    num_output: 471
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "loss"
  type: "SigmoidCrossEntropyLoss"
  bottom: "fc8-attributes"
  bottom: "labels"
  top: "loss"
  loss_param{
    ignore_label: -1

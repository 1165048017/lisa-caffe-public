name: "FinetuneVGG"

layer {
  name: "train-data"
  type: "Python"
  top: "images"
  top: "labels"
  python_param {
    module: "python_data_layers"
    layer: "captionClassifierImageData"
    param_str: "{'batch_size': 64, 'single_bit_classes': ['chair', 'truck', 'snowboard', 'broccoli', 'oven', 'bear', 'fork', 'vase'], 'json_images': 'utils_trainAttributes/imageJson_train_secondEight_471.json', 'images': 'utils_trainAttributes/imageTrain_coco_secondEight_471.txt', 'lexical_list': 'utils_trainAttributes/lexicalList_parseCoco_JJ100_NN300_VB100.txt', 'crop_dim': 224}"
    #param_str: "{'batch_size': 64, 'single_bit_classes': ['bottle', 'bus', 'couch', 'microwave', 'pizza', 'suitcase', 'racket', 'racquet', 'zebra'], 'json_images': 'utils_trainAttributes/imageJson_train_JJ155_NN511_VB100.json', 'images': 'utils_trainAttributes/imageTrain_coco.txt', 'lexical_list': 'utils_trainAttributes/lexicalList_parseCoco_JJ155_NN511_VB100.txt', 'crop_dim': 224}"
    #param_str: "{'batch_size': 64, 'single_bit_classes': ['bottle', 'bus', 'couch', 'microwave', 'pizza', 'suitcase', 'racket', 'racquet', 'zebra'], 'json_images': 'utils_trainAttributes/imageJson_train_JJ155_NN511_VB100.json', 'images': 'utils_trainAttributes/imageTrain_eightImagenet.txt', 'lexical_list': 'utils_trainAttributes/lexicalList_parseCoco_JJ155_NN511_VB100.txt', 'crop_dim': 224}"
    #param_str: "{'batch_size': 64, 'single_bit_classes': ['bottle', 'bus', 'couch', 'microwave', 'pizza', 'suitcase', 'racket', 'racquet', 'zebra'], 'json_images': 'utils_trainAttributes/imageJson_train.json', 'images': 'utils_trainAttributes/imageTrain_coco.txt', 'lexical_list': 'utils_trainAttributes/lexicalList_parseCoco_JJ100_NN300_VB100.txt', 'crop_dim': 224}"
    #param_str: "{'batch_size': 64, 'single_bit_classes': [], 'json_images': 'utils_trainAttributes/imageJson_train.json', 'images': 'utils_trainAttributes/imageTrain_coco.txt', 'lexical_list': 'utils_trainAttributes/lexicalList_parseCoco_JJ100_NN300_VB100.txt', 'crop_dim': 224}"
  }
  include { phase: TRAIN}
}
layer {
  name: "conv1_1"
  type: "Convolution"
  bottom: "images"
  top: "conv1_1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 1
  }

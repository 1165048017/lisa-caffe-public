net: "train_test_imageReadUPDATE.prototxt"
#test_iter: 1 
test_iter: 20
test_state: { stage: 'test-on-test' }
test_interval: 100
#test_net_reset: false
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 15000
